<?xml version="1.0" encoding="UTF-8"?>
<web-app metadata-complete="true" version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee                       http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <display-name>UTS-P4-Yudhi</display-name>
    <servlet>
        <servlet-name>FormMahasiswaServlet</servlet-name>
        <servlet-class>com.mardiana.yudhi.utsp4.MhsServlet.FormMahasiswaServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DataMahasiswa</servlet-name>
        <servlet-class>com.mardiana.yudhi.utsp4.MhsServlet.DataMahasiswa</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FormMahasiswaServlet</servlet-name>
        <url-pattern>/FormMahasiswaServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DataMahasiswa</servlet-name>
        <url-pattern>/DataMahasiswa</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>30</session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>DataMahasiswa</welcome-file>
    </welcome-file-list>
</web-app>
